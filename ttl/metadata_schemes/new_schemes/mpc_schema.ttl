<https://w3id.org/b4b/schemes/algorithms/mpc-schema#> a owl:Ontology ;
owl:imports <https://w3id.org/b4b/schemes/algorithms/sba#> ;
.

#################################################################
#     Classes
#################################################################

mpc:DSM-MPC
  rdfs:subClassOf sba:SmartBuildingAlgorithm;
  rdfs:label "DSM-MPC" ;
  rdfs:comment "Model Predictive Control Algorithm to optimize the charging behaviour of Electric Vehicles for Demand Side Management. Runs every x minutes from starting time to end time as defined by the scheduler." ;
.

mpc:OptimumSetpoints
  rdf:type owl:Class ;
  rdfs:label "OptimumSetpoints" ;
  rdfs:label "Optimum setpoints generated by the optimization algorithm." ;
  rdfs:subClassOf brick:Setpoint ;
.

mpc:MPCTimeResolution  a brick:Setpoint .

mpc:EVChargingBehaviourPrediction
  rdf:type owl:Class ;
  rdfs:label "EVChargingBehaviourPrediction" ;
  rdfs:subClassOf mpc:SystemBehaviourPrediction  ;
.

mpc:GridPowerIntakePrediction
  rdf:type owl:Class ;
  rdfs:label "GridPowerIntakePrediction" ;
  rdfs:subClassOf mpc:SystemBehaviourPrediction  ;
.

mpc:SystemBehaviourPrediction 
  rdf:type owl:Class ;
  rdfs:label "SystemBehaviourPrediction" ;
  rdfs:label "Predicted behaviour generated by MPC." ;
  rdfs:subClassOf sba:AlgorithmOutput ;
.

#################################################################
#     Object properties
#################################################################


mpc:hasInputDisturbanceForecast
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.

mpc:hasInputRealtimeFeedback
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.

mpc:hasInputConstraint
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.
mpc:hasInputSystemParameters
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.

mpc:hasOutputSystemBehaviourPrediction 
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasOutput 
.

mpc:hasOutputOptimumResults 
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasOutput
.