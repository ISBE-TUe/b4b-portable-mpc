:requiredClass_7b262822-b1e9-4400-bf95-7f9bbf5b7775
    a sh:NodeShape ;
    sh:targetNode :targetNode ;
    sh:sparql [
        a sh:SPARQLConstraint ;
        sh:message "The building must contain at least one instance of the data point." ;
        sh:select """
            SELECT ?this
            WHERE {
                FILTER NOT EXISTS {
                    ?sensor a brick-ev:EV_Battery_Size  .
                }
            }
        """ ;
    ] .
:requiedRelationship_6ba006c1-0242-4e89-ae76-bb9d2619802f
    a sh:NodeShape ;
    sh:targetClass brick-ev:EV_Battery_Size ;
    sh:property [
        sh:path ref:hasExternalReference;
        sh:minCount 1 ;
        sh:message "All datapoints need a timeseries reference." ;
    ] .
:reqioredExrefValue_8b6bc084-e487-4022-819f-b31cf4be81ca
    a sh:NodeShape ;
    sh:targetClass ref-ext:RealtimeTimeseriesReference ; 
    sh:property [
        sh:path ref-ext:hasRealtimeTimeseriesId;
        sh:datatype xsd:string ;    
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "All timeseries references need a value." ;
    ] .

