<https://w3id.org/b4b/schemes/algorithms/mpc-schema#> a owl:Ontology ;
owl:imports <https://w3id.org/b4b/schemes/algorithms/sba#> ;
.

#################################################################
#     Classes
#################################################################

mpc:OptimisationAlgorithm
  rdf:type owl:Class ;
  rdfs:label "OptimisationAlgorithm" ;
  rdfs:subClassOf sba:SmartBuildingAlgorithm ;
  rdfs:comment "Runs every x minutes from starting time to end time as defined by the scheduler." ;
.
mpc:Smart_Charging_MPC
  rdfs:subClassOf sba:OptimisationAlgorithm;
  rdfs:label "DSM-MPC" ;
  rdfs:comment "Model Predictive Control Algorithm to optimize the charging behaviour of Electric Vehicles for Demand Side Management. Runs every x minutes from starting time to end time as defined by the scheduler." ;
.
mpc:EVRealtimeFeedbackAlgorithm
  rdfs:subClassOf sba:RealtimeFeedbackAlgorithm;
  rdfs:label "EVRealtimeFeedbackAlgorithm" ;
  rdfs:comment "Realtime feedback algorithm for Electric Vehicle Charging Stations." ;
.

mpc:EVStateEstimationAlgorithm 
  rdf:type owl:Class ;
  rdfs:label "EVStateEstimationAlgorithm" ;
  rdfs:subClassOf sba:SmartBuildingAlgorithm ;
  rdfs:comment "Estimates the state of the Electric Vehicle Charging Station." ;
.

mpc:ParameterEstimationAlgorithm 
  rdf:type owl:Class ;
  rdfs:subClassOf sba:SmartBuildingAlgorithm ;
.
mpc:StaticParameterEstimationAlgorithm 
  rdf:type owl:Class ;
  rdfs:subClassOf mpc:ParameterEstimationAlgorithm ;
.
mpc:DynamicParameterEstimationAlgorithm 
  rdf:type owl:Class ;
  rdfs:subClassOf mpc:ParameterEstimationAlgorithm ;
.

mpc:EVChargingCurrentSetpoint 
    rdfs:label "EVChargingCurrentSetpoint" ;
    rdfs:comment "Setpoint for the Electric Vehicle Charging Current." ;
    rdfs:subClassOf mpc:OptimumSetpoints ;
.

mpc:OptimumSetpoints
  rdf:type owl:Class ;
  rdfs:label "OptimumSetpoints" ;
  rdfs:label "Optimum setpoints generated by the optimization algorithm." ;
  rdfs:subClassOf brick:Setpoint ;
.

mpc:EVChargingBehaviourPrediction
  rdf:type owl:Class ;
  rdfs:label "EVChargingBehaviourPrediction" ;
  rdfs:subClassOf mpc:SystemBehaviourPrediction  ;
.

mpc:GridPowerIntakePrediction
  rdf:type owl:Class ;
  rdfs:label "GridPowerIntakePrediction" ;
  rdfs:subClassOf mpc:SystemBehaviourPrediction  ;
.

mpc:SystemBehaviourPrediction 
  rdf:type owl:Class ;
  rdfs:label "SystemBehaviourPrediction" ;
  rdfs:label "Predicted behaviour generated by MPC." ;
  rdfs:subClassOf sba:AlgorithmOutput ;
.

#################################################################
#     Object properties
#################################################################


mpc:hasInputDisturbanceForecast
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.

mpc:hasInputRealtimeFeedback
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.

mpc:hasInputParameter  
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.

mpc:hasInputState 
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.

mpc:hasInputConstraint
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasInput 
.
# mpc:hasInputSystemParameters
#   a owl:ObjectProperty ;
#     rdfs:subPropertyOf sba:hasInput 
# .

mpc:hasSystemBehaviourPrediction 
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasOutput 
.

mpc:hasOutputOptimumResults 
  a owl:ObjectProperty ;
    rdfs:subPropertyOf sba:hasOutput
.

mpc:hasTimeResolution a owl:DatatypeProperty ;
    rdfs:domain sba:SmartBuildingAlgorithm ;
    rdfs:range xsd:string ;
.

mpc:hasPredictionHorizon a owl:DatatypeProperty ;
    rdfs:domain mpc:OptimisationAlgorithm ;
    rdfs:range xsd:string ;
.

mpc:MPCTimeResolution a owl:DatatypeProperty ;
    rdfs:domain mpc:OptimisationAlgorithm ;
    rdfs:range xsd:string ;
.
